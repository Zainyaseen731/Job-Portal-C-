@model zain.Models.job
@{
    ViewBag.Title = "Jobs";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Jobs that Available </h2>
<br />
<br />
@if (ViewBag.data != null)
{

    <table style="table-layout:auto;width:100%;">
        <tr>
            <th>Type</th>
            <th>Title</th>
            <th>Company</th>
            <th>Location</th>
        </tr>

        @foreach (var item in ViewBag.data)
        {

            <tr>
                <td>@item.jobtype</td>
                <td>@item.jobtitle</td>
                <td>@item.company</td>
                <td>@item.location</td>
                <td><a href="/Home/Edit/@item.Id">Edit</a></td>
            </tr>

        }


    </table>
    <br />
    <br />
}
